{
  "name": "@insightdesk/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file .env src/index.ts",
    "start": "tsx --env-file .env src/index.ts",
    "build": "tsc && tsc-alias --resolve-full-paths",
    "mcp": "tsx --env-file .env src/mcp/index.ts",
    "mcp:http": "cross-env MCP_TRANSPORT=http tsx --env-file .env src/mcp/index.ts",
    "mcp:config": "tsx --env-file .env scripts/generate-mcp-config.ts",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "setup": "tsx scripts/setup.ts",
    "db:generate": "node --env-file .env drizzle-kit generate",
    "db:migrate": "node --env-file .env drizzle-kit migrate",
    "db:push": "node --env-file .env drizzle-kit push",
    "db:studio": "node --env-file .env drizzle-kit studio",
    "db:seed": "tsx --env-file .env src/seed.ts",
    "db:reset": "tsx --env-file .env -e \"import { resetDatabase } from './src/lib/seed'; resetDatabase()\"",
    "test:seed": "tsx --env-file .env.test src/test/seed.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "test": "cross-env RUN_INTEGRATION_TESTS=true vitest run",
    "test:unit": "cross-env SKIP_INTEGRATION_TESTS=true vitest run --exclude '**/*.integration.test.ts'",
    "test:integration": "cross-env RUN_INTEGRATION_TESTS=true vitest run 'integration.test'",
    "test:watch": "cross-env SKIP_INTEGRATION_TESTS=true vitest --exclude '**/*.integration.test.ts'",
    "test:coverage": "cross-env SKIP_INTEGRATION_TESTS=true vitest run --coverage --exclude '**/*.integration.test.ts'",
    "test:ui": "cross-env SKIP_INTEGRATION_TESTS=true vitest --ui --exclude '**/*.integration.test.ts'",
    "test:setup": "tsx --env-file .env.test scripts/setup-integration-tests.ts",
    "test:containers:up": "docker compose -f docker-compose.test.yml up -d",
    "test:containers:down": "docker compose -f docker-compose.test.yml down",
    "test:containers:logs": "docker compose -f docker-compose.test.yml logs -f",
    "docs:generate": "tsx --env-file .env scripts/generate-openapi.ts",
    "db:setup:prod": "tsx scripts/setup-production-db.ts",
    "db:migrate:prod": "npx drizzle-kit migrate",
    "start:prod": "node dist/index.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/multer": "^2.0.0",
    "better-auth": "^1.4.4",
    "bullmq": "^5.65.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "iovalkey": "^0.3.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.11",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.7",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuidv7": "^1.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^22.19.1",
    "@types/nodemailer": "^6.4.21",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.14",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.7",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
